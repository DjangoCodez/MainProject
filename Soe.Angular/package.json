  "scripts": {
    "build": "tsc --version && npm run \"copy-i18n\" && gulp build",
    "transpile-less": "less-watch-compiler TypeScript build --run-once",
    "transpile-ts": "tsc --outDir build --sourceMap true --rootDir ./TypeScript",
    "watch-ts": "tsc --outDir build --sourceMap true --watch",
    "watch-views": "gulp watch-views",
    "watch": "npm run \"copy-i18n\" && concurrently \"npm run watch-ts\" \"gulp watch\"",
    "copy-i18n": "cpx \"TypeScript/Libs/i18n/**/*.*\" \"build/Libs/i18n/\"",
    "generateChecksums": "gulp generateChecksums"
